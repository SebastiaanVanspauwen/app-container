{
  "env": {
    "es2021": true,
    "node": true,
    "commonjs": true
  },
  "extends": [
    "appwise"
  ],
  "parserOptions": {
    "project": "tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/strict-boolean-expressions": "error",
    "max-len": "off",
    "curly": "off"
  },
  "ignorePatterns": [
    "jest.config.ts"
  ]
}
